<module>
  <inherits name='arez.component.Component'/>
  <inherits name='dagger.Dagger'/>

  <source path='' includes='*.java'/>
  <source path='spy'/>
  <super-source path='super'/>

  <define-property name='replicant.check_invariants' values='true,false'/>
  <set-property name='replicant.check_invariants' value='false'/>

  <define-property name='replicant.check_api_invariants' values='true,false'/>
  <set-property name='replicant.check_api_invariants' value='false'/>

  <define-property name='replicant.enable_names' values='true,false'/>
  <set-property name='replicant.enable_names' value='false'/>

  <define-property name='replicant.recordRequestKey' values='true,false'/>
  <set-property name='replicant.recordRequestKey' value='false'/>

  <define-property name='replicant.validateRepositoryOnLoad' values='true,false'/>
  <set-property name='replicant.validateRepositoryOnLoad' value='false'/>

  <define-property name='replicant.subscriptionsDebugOutputEnabled' values='true,false'/>
  <set-property name='replicant.subscriptionsDebugOutputEnabled' value='false'/>

  <define-property name='replicant.requestsDebugOutputEnabled' values='true,false'/>
  <set-property name='replicant.requestsDebugOutputEnabled' value='false'/>

  <define-property name='replicant.enable_spies' values='true,false'/>
  <set-property name='replicant.enable_spies' value='false'/>

  <define-property name='replicant.enable_zones' values='true,false'/>
  <set-property name='replicant.enable_zones' value='false'/>

  <define-property name='replicant.logger' values='console_js,jul,proxy,none'/>
  <set-property name='replicant.logger' value='console_js'/>
</module>
