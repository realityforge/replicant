<module>
  <inherits name="org.realityforge.replicant.Replicant"/>

  <source path=''>
    <exclude name='**/*'/>
  </source>

  <set-property name="replicant.shouldRecordRequestKey" value="true"/>
  <set-property name="replicant.shouldValidateRepositoryOnLoad" value="true"/>
  <set-property name="replicant.repositoryDebugOutputEnabled" value="true"/>
  <set-property name="replicant.subscriptionsDebugOutputEnabled" value="true"/>
  <set-property name="replicant.requestDebugOutputEnabled" value="true"/>
</module>
