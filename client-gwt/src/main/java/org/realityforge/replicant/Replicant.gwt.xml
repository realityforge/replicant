<?xml version="1.0" encoding="UTF-8"?>
<module>
  <inherits name="org.realityforge.replicant.ReplicantClientCommon"/>
  <inherits name="dagger.Dagger" />
  <inherits name="com.google.gwt.storage.Storage"/>
  <inherits name="com.google.gwt.user.Window"/>
  <inherits name="com.google.gwt.http.HTTP"/>
  <inherits name="com.google.gwt.json.JSON"/>

  <source path='client/gwt'/>
  <source path='client/runtime/gwt'/>
  <source path='client/net/gwt'/>

  <super-source path="sprsrc" />

  <define-property name="replicant.shouldRecordRequestKey" values="true,false"/>
  <set-property name="replicant.shouldRecordRequestKey" value="false"/>

  <define-property name="replicant.shouldValidateRepositoryOnLoad" values="true,false"/>
  <set-property name="replicant.shouldValidateRepositoryOnLoad" value="false"/>

  <define-property name="replicant.repositoryDebugOutputEnabled" values="true,false"/>
  <set-property name="replicant.repositoryDebugOutputEnabled" value="false"/>

  <define-property name="replicant.subscriptionsDebugOutputEnabled" values="true,false"/>
  <set-property name="replicant.subscriptionsDebugOutputEnabled" value="false"/>

  <define-property name="replicant.requestDebugOutputEnabled" values="true,false"/>
  <set-property name="replicant.requestDebugOutputEnabled" value="false"/>
</module>
